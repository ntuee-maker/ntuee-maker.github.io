{
  "name": "ntuee-maker.github.io",
  "description": "NTUEE makerspace's website",
  "version": "1.0.0",
  "author": "Jason Jian",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "yarn clean && gatsby develop --host 0.0.0.0 --port 8000",
    "build": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "lint:js": "eslint . && flow check .",
    "lint:md": "remark -q .",
    "clean": "bash ./clean.sh",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "deploy": "yarn build && gh-pages -b master -d public -m '☄️ Deploy'"
  },
  "dependencies": {
    "autoprefixer": "^8.4.1",
    "gatsby": "^1.9.259",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-postcss-sass": "^1.0.20",
    "gatsby-plugin-sharp": "^1.6.46",
    "gatsby-source-filesystem": "^1.5.35",
    "gatsby-transformer-remark": "^1.7.40",
    "gatsby-transformer-sharp": "^1.6.26"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.2.3",
    "gh-pages": "^1.1.0",
    "remark-cli": "^5.0.0",
    "remark-frontmatter": "^1.2.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "rimraf": "^2.6.2"
  }
}
