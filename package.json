{
  "name": "ntuee-maker.github.io",
  "description": "NTUEE makerspace's website",
  "version": "1.0.0",
  "author": "Jason Jian",
  "scripts": {
    "dev": "yarn clean && gatsby develop",
    "build": "yarn clean && gatsby build",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "lint:js": "eslint . && flow check .",
    "clean": "bash ./clean.sh",
    "deploy": "yarn build && gh-pages -b master -d public -m '☄️ Deploy'"
  },
  "dependencies": {
    "autoprefixer": "^8.4.1",
    "gatsby": "^1.9.158",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-postcss-sass": "^1.0.20",
    "gatsby-source-filesystem": "^1.5.24",
    "gatsby-transformer-remark": "^1.7.33"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.0",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.2.3",
    "gh-pages": "^1.1.0",
    "rimraf": "^2.6.2"
  }
}
